{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactoryengine6554"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorageSInk_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageSInk'"
		},
		"AzureDataLakeStoragesinkSide_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStoragesinkSide'"
		},
		"AzureDataLakeStoragesoucer_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStoragesoucer'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://deltalake123.dfs.core.windows.net/"
		},
		"AzureDataLakeStorageSInk_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakegen2sink.dfs.core.windows.net/"
		},
		"AzureDataLakeStoragesinkSide_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakegen2sink.dfs.core.windows.net/"
		},
		"AzureDataLakeStoragesoucer_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://deltalake123.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactoryengine6554_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorageSInk_accountKey": {
						"value": "[parameters('AzureDataLakeStorageSInk_accountKey')]"
					},
					"AzureDataLakeStoragesinkSide_accountKey": {
						"value": "[parameters('AzureDataLakeStoragesinkSide_accountKey')]"
					},
					"AzureDataLakeStoragesoucer_accountKey": {
						"value": "[parameters('AzureDataLakeStoragesoucer_accountKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorageSInk_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageSInk_properties_typeProperties_url')]"
					},
					"AzureDataLakeStoragesinkSide_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStoragesinkSide_properties_typeProperties_url')]"
					},
					"AzureDataLakeStoragesoucer_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStoragesoucer_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}